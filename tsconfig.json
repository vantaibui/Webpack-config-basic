{
    "compilerOptions": {
      "outDir": "./dist/",
      "noImplicitAny": true,
      "sourceMap": true,
      "module": "es6",
      "target": "es5",
      "jsx": "react",
      "allowJs": true,
      "moduleResolution": "node",
      // Resolve default import module
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
    }
  }